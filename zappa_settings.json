{
    "dev": {
        "app_function": "proyecto.lambda_handler",
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "project_name": "environment",
        "runtime": "python3.10",
        "s3_bucket": "zappa-zoa6q2awf",
        "keep_warm": false,
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "LabRole",
        "events": [ 
          { 
            "function": "proyecto.lambda_handler", 
            "expression": "cron(0 14 * * ? *)"
            
          } 
        ]
    },
    
    
    "dev2": {
        "app_function": "transformacion.lambda_handler",
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "project_name": "paso2",
        "runtime": "python3.10",
        "s3_bucket": "zappa-zoa6q2awf",
        "keep_warm": false,
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "LabRole",
        "events": [
        {
            "function": "transformacion.lambda_handler",
            "event_source": {
              "arn":  "arn:aws:s3:::paginaspruebas",
              "events": [
                "s3:ObjectCreated:*"
              ]
           }
        }]
    }
}